@model EmpViewModel


<div class="form-group">
    <label asp-for="Name" for="Name" class="form-label"></label>
    <input asp-for="Name" id="Name" class="form-control" readonly>
</div>
<div class="form-group">
    <label asp-for="Age" for="Age" class="form-label"></label>
    <input asp-for="Age" id="Age" class="form-control" readonly>
    <span asp-validation-for="Age"></span>
</div>
<div class="form-group">
    <label asp-for="Salary" for="Salary" class="form-label"></label>
    <input asp-for="Salary" id="Salary" class="form-control" readonly>
</div>
<div class="form-group">
    <label asp-for="Email" for="Email" class="form-label"></label>
    <input type="email" asp-for="Email" id="Email" class="form-control" readonly>
</div>
